<templateSet group="PHP">
  <template name="prepr" value="echo '&lt;pre&gt;', print_r($END$, true), '&lt;/pre&gt;';" description="&lt;pre&gt; print" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="nspace" value="namespace $NAMESPACE$" description="Add the current namespace" toReformat="true" toShortenFQNames="true">
    <variable name="NAMESPACE" expression="phpQualifiedClassName()" defaultValue="App\" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>